<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <Thickness x:Key="ThickWidth">2</Thickness>
    <Thickness x:Key="ThinWidth">1</Thickness>
    <system:Double x:Key="ThinWidthDouble">1</system:Double>
    <system:Double x:Key="ThickWidthDouble">2</system:Double>
    
    <system:Double x:Key="IconFontSize">24</system:Double>
    <system:Double x:Key="DefaultFontSize">20</system:Double>
    <system:Double x:Key="LargeFontSize">30</system:Double>
    <CornerRadius x:Key="TextBoxCornerRadius">10</CornerRadius>
</ResourceDictionary>
